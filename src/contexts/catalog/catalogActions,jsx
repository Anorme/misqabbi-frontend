import { SET_PRODUCTS, SET_FILTER, SET_PAGE } from "./catalogActionTypes";

export const setProducts = (products) => ({
  type: SET_PRODUCTS,
  payload: products,
});

export const setFilter = (filter) => ({
  type: SET_FILTER,
  payload: filter,
});

export const setPage = (page) => ({
  type: SET_PAGE,
  payload: page,
});
